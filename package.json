{
	"name": "@jennynbevent",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --port 5176",
		"build": "vite build",
		"build:local": "dotenv -e .env.local -- vite build",
		"preview": "vite preview --host",
		"preview:local": "dotenv -e .env.local -- vite preview --host",
		"preview:network": "vite preview --host --port 4173",
		"preview:local:network": "dotenv -e .env.local -- vite preview --host --port 4173",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:watch": "vitest",
		"test": "vitest run",
		"lint": "eslint .",
		"format": "prettier --write . --ignore-path .gitignore",
		"format:check": "prettier --check . --ignore-path .gitignore",
		"geocode:shops": "tsx scripts/geocode-shops.ts",
		"geocode:shops:prod": "NODE_ENV=production tsx scripts/geocode-shops.ts",
		"create:test-shops": "tsx scripts/create-test-shops.ts",

		"db:reset:staging": "dotenv -e .env.local -- tsx scripts/reset-db-from-prod.ts staging",
		"db:sync:local": "dotenv -e .env.local -- tsx scripts/sync-migrations.ts local",
		"db:sync:staging": "dotenv -e .env.local -- tsx scripts/sync-migrations.ts staging",
		"db:sync:prod": "dotenv -e .env.local -- tsx scripts/sync-migrations.ts production",
		"db:reset:staging:history": "dotenv -e .env.local -- tsx scripts/reset-migration-history.ts staging",
		"db:reset:prod:history": "dotenv -e .env.local -- tsx scripts/reset-migration-history.ts production",
		"db:add-initial-migration:prod": "dotenv -e .env.local -- tsx scripts/add-initial-migration-to-history.ts production",
		"db:add-initial-migration:staging": "dotenv -e .env.local -- tsx scripts/add-initial-migration-to-history.ts staging",
		"validate:migration": "tsx scripts/validate-migration-safety.ts",
		"validate:migrations": "tsx scripts/validate-migration-safety.ts all",
		"backup:before-migration": "dotenv -e .env.local -- tsx scripts/backup-before-migration.ts",
		"resend:sync-pastries": "dotenv -e .env.local -- tsx scripts/sync-all-pastries-to-resend.ts",
		"send:campaign": "dotenv -e .env.local -- tsx scripts/send-marketing-campaign.ts"
